{
  "routes": [
    {
      "pattern": "/sites/:id",
      "filePath": "/sites/main.txt"
    },
    {
      "pattern": "/replacement-phone-blocks/:id(\\d+|-\\d+)",
      "filePath": "/replacement-phone-blocks/main.txt"
    },
    {
      "id": "test_id",
      "pattern": "/marketplace/(.*)",
      "filePath": "/callTracking/rules/step1.txt"
    },
    {
      "pattern": "/call-tracking/:type(birds|fish)",
      "filePath": "/callTracking/numbers.txt"
    },
    {
      "pattern": "/call-tracking/rules/step",
      "filePath": "/callTracking/rules/step1.txt"
    },
    {
      "regExp": "^/marketplace/(-?\\d+)$",
      "filePath": "/callTracking/rules/step1.txt"
    },
    {
      "pattern": "/qwe",
      "filePath": "/callTracking/rules/step1.txt",
      "exact": true
    },
    {
      "pattern": "/summary_analytics/:id",
      "filePath": "/analytics/summary_analytics.txt",
      "conditions": {
        "and": [
          {
            "parameter": "app_id",
            "operator": "=",
            "value": 4735
          },
          {
            "parameter": "lang",
            "operator": "=",
            "value": "ru"
          },
          {
            "or": [
              {
                "parameter": "components",
                "operator": "include",
                "value": "operation"
              },
              {
                "parameter": "components",
                "operator": "include",
                "value": "asdqwe"
              }
            ]
          }
        ]
      }
    },
    {
      "pattern": "/ad_analytics/:id",
      "filePath": "/analytics/summary_analytics.txt",
      "style": "megafon",
      "conditions": {
        "and": [
          {
            "parameter": "app_id",
            "operator": "=",
            "value": 4735
          },
          {
            "parameter": "lang",
            "operator": "=",
            "value": "en"
          },
          {
            "or": [
              {
                "parameter": "components",
                "operator": "include",
                "value": "operation"
              },
              {
                "parameter": "components",
                "operator": "include",
                "value": "asdqwe"
              }
            ]
          }
        ]
      }
    }
  ]
}